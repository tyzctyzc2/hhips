<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
  <class name = "db.ProblemByPaper" table = "v_paperproblem">
     
     <meta attribute = "class-description">
        This class contains the problem detail. 
     </meta>
     
     <id name = "paperproblemid" type = "int" column = "paperproblemid">
        <generator class="native"/>
     </id>
     
     <property name = "idproblem" column = "idproblem" type = "int"/>
     <property name = "problemlevel" column = "problemlevel" type = "int"/>
     <property name = "problemchapterid" column = "problemchapterid" type = "int"/>
     <property name = "problemindex" column = "problemindex" type = "string"/>
     <property name = "problemmodule" column = "problemmodule" type = "int"/>
     <property name = "problemcisactive" column = "problemcisactive" type = "int"/>
     <property name = "problemanswerstring" column = "problemanswerstring" type = "string"/>
     <property name = "problemdetail" column = "problemdetail" type = "string"/>
     <property name = "problemanswerdetail" column = "problemanswerdetail" type = "string"/>
     <property name = "problemstatus" column = "problemstatus" type = "int"/>
     <property name = "papername" column = "papername" type = "string"/>
     <property name = "idpaper" column = "idpaper" type = "int"/>
     <property name = "isactive" column = "isactive" type = "int"/>
     <property name = "usedtime" column = "usedtime" type = "int"/>
     <property name = "workmark" column = "workmark" type = "int"/>
       <property name="idwork" type="int">
            <column name="idwork" />
        </property>
        <property name="workdetail" type="java.lang.String">
            <column name="workdetail" />
        </property>
     
    
     
  </class>
</hibernate-mapping>